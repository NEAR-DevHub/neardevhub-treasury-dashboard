{
  "name": "neardevhub-trustees",
  "version": "1.0.0",
  "description": "Trustees dashboard built for NEAR BOS",
  "type": "module",
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "bos-cli": "^0.3.11",
    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "near-social-vm-types": "^1.0.0",
    "prettier": "^2.8.4",
    "replace-in-files": "^3.0.0",
    "rimraf": "^5.0.1",
    "sucrase": "^3.34.0",
    "tsc-files": "^1.1.4",
    "@playwright/test": "^1.41.1",
    "near-bos-webcomponent": "^0.0.2",
    "http-server": "^14.1.1"
  },
  "scripts": {
    "fmt": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "fmt:check": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "tsc --pretty --noEmit && eslint .",
    "dev": "~/.cargo/bin/bos-loader treasurydevhub.testnet --path ./.bos/transpiled/src",
    "build": "npm run fmt && rimraf .bos/transpiled && mkdir -p .bos/transpiled/src && sucrase ./src -d .bos/transpiled/src/neardevhub-trustees --transforms typescript,jsx --jsx-runtime preserve --disable-es-transforms --out-extension jsx && sucrase ./src -d .bos/transpiled/src/neardevhub-trustees --transforms jsx --jsx-runtime preserve --disable-es-transforms --out-extension jsx && node ./build.js",
    "deploy": "npm run build && cd .bos/transpiled && bos components deploy",
    "prepare": "husky install",
    "gateway": "node scripts/dev-gateway.js",
    "test": "npx playwright test",
    "test:watch:codespaces": "npm test -- --ui-host=0.0.0.0",
    "build:watch": "node ./scripts/watch.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,json}": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NEAR-DevHub/neardevhub-treasury-dashboard.git"
  },
  "keywords": [
    "near",
    "nearprotocol",
    "bos",
    "blockchains",
    "typescript",
    "starter"
  ],
  "author": "devhub.near",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NEAR-DevHub/neardevhub-treasury-dashboard.git"
  },
  "homepage": "https://github.com/NEAR-DevHub/neardevhub-treasury-dashboard.git#readme"
}
